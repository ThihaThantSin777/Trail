<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vb" styleClass="loginView" stylesheets="@layout.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChooseViewController">
   <children>
      <VBox>
         <children>
            <Label text="Choose one" />
         </children>
         <styleClass>
            <String fx:value="loginTitle" />
            <String fx:value="boldFont" />
            <String fx:value="lightGreen" />
         </styleClass>
      </VBox>
      <FlowPane prefHeight="254.0" prefWidth="500.0">
         <children>
            <JFXButton onAction="#twoDM" text="2D (M)">
               <styleClass>
                  <String fx:value="chooseButton" />
                  <String fx:value="chooseFont" />
                  <String fx:value="h2" />
                  <String fx:value="lightGreen" />
               </styleClass>
            </JFXButton>
            <JFXButton onAction="#twoDE" text="2D (E)">
               <styleClass>
                  <String fx:value="chooseButton" />
                  <String fx:value="chooseFont" />
                  <String fx:value="h2" />
                  <String fx:value="lightGreen" />
               </styleClass>
            </JFXButton>
            <JFXButton onAction="#threeD" text="3D">
               <styleClass>
                  <String fx:value="chooseButton" />
                  <String fx:value="chooseFont" />
                  <String fx:value="h2" />
                  <String fx:value="lightGreen" />
               </styleClass>
            </JFXButton>
         </children>
         <styleClass>
            <String fx:value="loginBody" />
            <String fx:value="gap" />
         </styleClass>
      </FlowPane>
   </children>
</VBox>
